{
  "Arrow Anonymous Function": {
    "prefix": "anfn",
    "body": "() => {}"
  },

  "Async Arrow Anonymous Function": {
    "prefix": "aanfn",
    "body": "async () => {}"
  },

  "Console Log Variable": {
    "prefix": "clo",
    "body": "console.log('${1:variable} :>>', ${1:variable})"
  },

  "Console Log": {
    "prefix": "clog",
    "body": "console.log('${1:message}')"
  },

  "Private Readonly Property": {
    "prefix": "pro",
    "body": "private readonly ${1:property}"
  }
}
